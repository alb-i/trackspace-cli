{
  "compilerOptions": {
    "target":"ESNext",
    "module":"ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSourceMap": true,
    "outDir": "dist"
  },
  "ts-node": {
    "esm": false
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}